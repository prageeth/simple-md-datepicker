{
  "name": "@prageeths/simple-md-datepicker",
  "version": "0.12.0",
  "description": "A Vanilla JS Material Design date/time picker component",
  "main": "dist/simple-md-datepicker.js",
  "module": "dist/simple-md-datepicker.mjs",
  "jsnext:main": "dist/simple-md-datepicker.mjs",
  "scripts": {
    "build": "rollup --config",
    "build:watch": "rollup --config -w",
    "clean": "rm -r dist/*",
    "lint": "eslint lib/**/*.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prageeth/simple-md-datepicker.git"
  },
  "author": "@prageeths",
  "contributors": [
    "Prageeth Silva <prageethsilva@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "rome": "https://github.com/jwhitfieldseed/rome.git#19f5d3031a922c29c52b9038b2832a827e5e99d6"
  },
  "peerDependencies": {
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babelrc-rollup": "^3.0.0",
    "delay": "^1.3.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.1.0",
    "node-sass": "^4.3.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-sass": "^0.4.8",
    "rollup-watch": "^3.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/lib/js/__tests__/helpers/empty-module.js"
    },
    "testPathIgnorePatterns": [
      "/helpers/"
    ],
    "setupFiles": [
      "<rootDir>/lib/js/__tests__/helpers/dom-polyfills.js"
    ]
  }
}
